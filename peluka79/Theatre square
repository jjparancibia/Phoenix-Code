#include<iostream>
using namespace std;
int main()
{
    long long n,m,a;
	int i=1,k=1;
	 
	
	cin>>n;
	cin>>m;
	cin>>a;
	
	while(i<=9)
	{
		k=k*10;
		i++;
	}
	
	if(n>=1 && m>=1 && a>=1 && n<=k && m<=k && a<=k)
	   {
        if(m == n)
		  {
		   	if(a>=m)
			  {	cout<<"1";}
			else if(a<m)
			  { 
			    if(m%a != 0)
			    {cout<<(((m/a)+1)*((m/a)+1));}
			    else if(m%a == 0)
			    {cout<<((m/a)*(m/a));}
			  } 
		  }
		else if(m != n) 
		{
		  if(m < n)
		  {
		  	if(a>=n)
		  	{cout<<"1";}
		  	else if(a>=m && a<n)
		  	{
			  if(n%a !=0 )
			  {cout<<(n/a)+1;}
			  else if(n%a == 0)
			  {cout<<(n/a);}
		    }
			else if(a<m && a<n)
			{
			   if(m%a != 0 && n%a !=0)
			    {cout<<(((m/a)+1)*((n/a)+1));}
			   else if(m%a == 0 && n%a !=0)
			    {cout<<((m/a)*((n/a)+1));}
			   else if(m%a == 0 && n%a ==0)
			    {cout<<((m/a)*(n/a));}
			   else if(m%a != 0 && n%a ==0)
			    {cout<<(((m/a)+1)*(n/a));}
			   
			   
			}
		  }
		  else if(m > n)
		  {
		  	if(a>=m)
		  	{cout<<"1";}
		  	else if(a>=n && a<m)
		  	{if(m%a !=0 )
			  {cout<<(m/a)+1;}
			  else if(m%a == 0)
			  {cout<<(m/a);}
		    }
			else if(a<n && a<m)
			{
			   if(n%a != 0 && m%a !=0)
			    {cout<<(((n/a)+1)*((m/a)+1));}
			   else if(n%a == 0 && m%a !=0)
			    {cout<<((n/a)*((m/a)+1));}
			   else if(n%a == 0 && m%a ==0)
			    {cout<<((n/a)*(m/a));}
			   else if(n%a != 0 && m%a ==0)
			    {cout<<(((n/a)+1)*(m/a));}
			
			}
		  }
	     }
		}	   	
	   	
	
	system("pause");
	return 0;
}
